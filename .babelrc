// 按需引入element-ui 文件配置，先npm install babel-plugin-component -D
{
	 "presets": [
      ["env",{
          "modules": false,
          "targets": {
          "browsers": ["> 1%",
          "last 2 versions",
          "not ie <= 8"]   
        }  
      }],
      ["es2015",{
        "modules": false
      }],
      "stage-2",
    ],
	 "plugins": ["transform-vue-jsx",
	   "transform-runtime",
	   ["component",{
		     "libraryName": "element-ui",
		     "styleLibraryName": "theme-chalk"      
      }   
    ]]
}